{
  "name": "Sugly",
  "scopeName": "source.sugly",
  "fileTypes": [
    "sugly",
    "s"
  ],
  "foldingStartMarker": "\\(\\s*$",
  "foldingStopMarker": "^\\s*\\)[\\.\\,]?",
  "patterns": [
    {
      "name": "comment.line.number-sign.sugly",
      "match": "(#).*$\\n?",
      "captures": {
        "1": {
          "name": "punctuation.definition.comment.sugly"
        }
      }
    },
    {
      "name": "keyword.other.punctuation.sugly",
      "match": "[\\:\\@\\`\\$]"
    },
    {
      "name": "meta.keyword-quote-sugly",
      "match": "\\(\\s*(quote)\\b",
      "captures": {
        "1": {
          "name": "keyword.other.quote.sugly"
        }
      }
    },
    {
      "name": "meta.keyword-assignment-sugly",
      "match": "\\(\\s*(let|var|local|export|import|include|load)\\b",
      "captures": {
        "1": {
          "name": "keyword.other.assignment.sugly"
        }
      }
    },
    {
      "name": "meta.control-flow-sugly",
      "match": "\\b(if|for|while|break|continue)\\b",
      "captures": {
        "1": {
          "name": "keyword.control.flow.sugly"
        }
      }
    },
    {
      "name": "meta.control-preposition-sugly",
      "match": "\\b(else|in)\\b",
      "captures": {
        "1": {
          "name": "keyword.control.preposition.sugly"
        }
      }
    },
    {
      "name": "meta.control-procedure-sugly",
      "match": "\\b(redo|return|exit)\\b",
      "captures": {
        "1": {
          "name": "keyword.control.procedure.sugly"
        }
      }
    },
    {
      "name": "meta.function-self-sugly",
      "match": "\\b(do)\\b",
      "captures": {
        "1": {
          "name": "support.function.self.sugly"
        }
      }
    },
    {
      "name": "meta.variable-language-sugly",
      "match": "\\b(this|arguments|that|operation|operand)\\b",
      "captures": {
        "1": {
          "name": "variable.language.sugly"
        }
      }
    },
    {
      "name": "meta.type-generic-sugly",
      "match": "\\b(type|bool|number|date|string|range|symbol|tuple|operator|lambda|function|array|object|class)\\b",
      "captures": {
        "1": {
          "name": "storage.type.generic.sugly"
        }
      }
    },
    {
      "name": "meta.constant-language-sugly",
      "match": "\\b(null|true|false)\\b",
      "captures": {
        "1": {
          "name": "constant.language.sugly"
        }
      }
    },
    {
      "name": "meta.operator-operator-decl-sugly",
      "match": "\\(\\s*(\\=\\?)(?=\\(|\\)|\\s)",
      "captures": {
        "1": {
          "name": "keyword.other.operator-decl.sugly"
        }
      }
    },
    {
      "name": "meta.operator-lambda-decl-sugly",
      "match": "\\(\\s*(\\=[\\>]?)(?=\\(|\\)|\\:|\\s)",
      "captures": {
        "1": {
          "name": "support.function.lambda-decl.sugly"
        }
      }
    },
    {
      "name": "meta.operator-global-sugly",
      "match": "\\(\\s*(\\+|\\?|\\!|not|\\~)(?=\\(|\\)|\\s)",
      "captures": {
        "1": {
          "name": "keyword.other.global.sugly"
        }
      }
    },
    {
      "name": "meta.operator-double-sugly",
      "match": "\\(\\s*(\\+\\+|--)(?=\\(|\\)|\\:|\\s)",
      "captures": {
        "1": {
          "name": "keyword.other.double.sugly"
        }
      }
    },
    {
      "name": "meta.operator-general-sugly",
      "match": "(?:\\)|\\s)(\\?\\??|\\&\\&|\\|\\|)(?=\\(|\\)|\\s)",
      "captures": {
        "1": {
          "name": "keyword.other.general.sugly"
        }
      }
    },
    {
      "name": "meta.operator-arithmetic-sugly",
      "match": "(?:\\)|\\s)(\\+[\\+\\=]?|\\-[\\-\\=]?|\\*[\\=]?|\\/[\\=]?)(?=\\(|\\)|\\s)",
      "captures": {
        "1": {
          "name": "keyword.other.arithmetic.sugly"
        }
      }
    },
    {
      "name": "meta.operator-bitwise-sugly",
      "match": "(?:\\)|\\s)(\\&[\\=]?|\\|[\\=]?|\\^[\\=]?|\\<\\<[\\=]?|\\>{2,3}[\\=]?)(?=\\(|\\)|\\s)",
      "captures": {
        "1": {
          "name": "keyword.other.bitwise.sugly"
        }
      }
    },
    {
      "name": "meta.operator-comparison-sugly",
      "match": "(?:\\)|\\s)([\\=\\!][\\=]|[\\>\\<][\\=]?)(?=\\(|\\)|\\s)",
      "captures": {
        "1": {
          "name": "keyword.other.sugly"
        }
      }
    },
    {
      "name": "meta.function-entity-sugly",
      "match": "(?:\\)|\\s)(is-not|is|equals|not-equals|compare|is-empty|not-empty|is-a|is-not-a|to-code|to-string)(?=\\)|\\(|\\s)",
      "captures": {
        "1": {
          "name": "support.function.sugly"
        }
      }
    },
    {
      "name": "meta.function-type-sugly",
      "match": "(?:\\)|\\s)(of|objectify|typify)(?=\\)|\\(|\\s)",
      "captures": {
        "1": {
          "name": "support.function.sugly"
        }
      }
    },
    {
      "name": "meta.constant-empty-sugly",
      "match": "(?:\\()\\s*(type|bool|string|number|date|range|symbol|tuple)\\s+(empty)\\b",
      "captures": {
        "1": {
          "name": "storage.type.generic.sugly"
        },
        "2": {
          "name": "support.constant.empty.sugly"
        }
      }
    },
    {
      "name": "meta.function-empty-sugly",
      "match": "(?:\\()\\s*(operator|lambda|function|array|object|class)\\s+(empty)\\b",
      "captures": {
        "1": {
          "name": "storage.type.generic.sugly"
        },
        "2": {
          "name": "support.function.empty.sugly"
        }
      }
    },
    {
      "name": "meta.function-other-empty-sugly",
      "match": "(?:\\)|\\s)(empty)\\b",
      "captures": {
        "1": {
          "name": "support.function.other.empty.sugly"
        }
      }
    },
    {
      "name": "meta.operator-noop-sugly",
      "match": "(?:\\()\\s*(operator)\\s+(noop)\\b",
      "captures": {
        "1": {
          "name": "storage.type.generic.sugly"
        },
        "2": {
          "name": "keyword.other.operator_.noop.sugly"
        }
      }
    },
    {
      "name": "meta.lambda-noop-sugly",
      "match": "(?:\\()\\s*(lambda|function)\\s+(noop)\\b",
      "captures": {
        "1": {
          "name": "storage.type.generic.sugly"
        },
        "2": {
          "name": "support.function.noop.sugly"
        }
      }
    },
    {
      "name": "meta.function-collection-sugly",
      "match": "(?:\\)|\\s)(length|first-of|first|last-of|last|copy|slice|concat)\\b",
      "captures": {
        "1": {
          "name": "support.function.collection.sugly"
        }
      }
    },
    {
      "name": "meta.function-string-sugly",
      "match": "(?:\\)|\\s)(of-chars|starts-with|ends-with|trim-left|trim-right|trim|replace|to-upper|to-lower|split|char-at)\\b",
      "captures": {
        "1": {
          "name": "support.function.string.sugly"
        }
      }
    },
    {
      "name": "meta.constant-number-sugly",
      "match": "(?:\\()\\s*(number)\\s+(max-int|min-int|max-bits|min-bits|infinite|-infinite|max|min|bits|invalid|smallest)\\b",
      "captures": {
        "1": {
          "name": "storage.type.generic.sugly"
        },
        "2": {
          "name": "support.constant.number.sugly"
        }
      }
    },
    {
      "name": "meta.function-number-type-sugly",
      "match": "(?:\\()\\s*(number)\\s+(parse-int|parse|of-int|of-bits)\\b",
      "captures": {
        "1": {
          "name": "storage.type.generic.sugly"
        },
        "2": {
          "name": "support.function.number.type.sugly"
        }
      }
    },
    {
      "name": "meta.function-number-sugly",
      "match": "(?:\\)|\\s)+(is-valid|is-invalid|is-int|is-not-int|is-bits|is-not-bits|is-finite|is-infinite|as-int|as-bits|and|minus|times|divided-by|ceil|floor|round|trunc)\\b",
      "captures": {
        "1": {
          "name": "support.function.number.sugly"
        }
      }
    },
    {
      "name": "meta.constant-date-sugly",
      "match": "(?:\\()\\s*(date)\\s+(invalid)\\b",
      "captures": {
        "1": {
          "name": "storage.type.generic.sugly"
        },
        "2": {
          "name": "support.constant.date.sugly"
        }
      }
    },
    {
      "name": "meta.function-date-type-sugly",
      "match": "(?:\\()\\s*(date)\\s+(parse|of-utc|now|timestamp|timezone)\\b",
      "captures": {
        "1": {
          "name": "storage.type.generic.sugly"
        },
        "2": {
          "name": "support.function.date.type.sugly"
        }
      }
    },
    {
      "name": "meta.function-date-sugly",
      "match": "(?:\\)|\\s)+(date-fields|time-fields|all-fields|week-day|timestamp)\\b",
      "captures": {
        "1": {
          "name": "support.function.date.sugly"
        }
      }
    },
    {
      "name": "meta.constant-symbol-sugly",
      "match": "(?:\\()\\s*(symbol)\\s+(invalid|etc|all|any|quote|lambda|function|operator|let|var|local|export|import|include|load|begin|end|comma|semicolon|period|object|pairing|comment|in|else)\\b",
      "captures": {
        "1": {
          "name": "storage.type.generic.sugly"
        },
        "2": {
          "name": "support.constant.symbol.sugly"
        }
      }
    },
    {
      "name": "meta.function-symbol-type-sugly",
      "match": "(?:\\()\\s*(symbol)\\s+(of-shared)\\b",
      "captures": {
        "1": {
          "name": "storage.type.generic.sugly"
        },
        "2": {
          "name": "support.function.symbol.type.sugly"
        }
      }
    },
    {
      "name": "meta.constant-tuple-sugly",
      "match": "(?:\\()\\s*(tuple)\\s+(blank|unknown|lambda|function|operator|array|object)\\b",
      "captures": {
        "1": {
          "name": "storage.type.generic.sugly"
        },
        "2": {
          "name": "support.constant.tuple.sugly"
        }
      }
    },
    {
      "name": "meta.function-tuple-type-sugly",
      "match": "(?:\\()\\s*(tuple)\\s+(accepts|atom-of|of-plain|from-plain|from)\\b",
      "captures": {
        "1": {
          "name": "storage.type.generic.sugly"
        },
        "2": {
          "name": "support.function.tuple.type.sugly"
        }
      }
    },
    {
      "name": "meta.function-tuple-sugly",
      "match": "(?:\\)|\\s)+(is-plain|not-plain|as-plain|source-map|iterate|merge|to-array)\\b",
      "captures": {
        "1": {
          "name": "support.function.tuple.sugly"
        }
      }
    },
    {
      "name": "meta.function-operation-sugly",
      "match": "(?:\\)|\\s)+(parameters|body|is-generic|not-generic|apply)\\b",
      "captures": {
        "1": {
          "name": "support.function.operation.sugly"
        }
      }
    },
    {
      "name": "meta.function-array-type-sugly",
      "match": "(?:\\()\\s*(array)\\s+(from)\\b",
      "captures": {
        "1": {
          "name": "storage.type.generic.sugly"
        },
        "2": {
          "name": "support.function.array.type.sugly"
        }
      }
    },
    {
      "name": "meta.function-array-sugly",
      "match": "(?:\\)|\\s)+(count|append|get|set|reset|remove|clear|has|swap|splice|pop|push|reverse|sort|join)\\b",
      "captures": {
        "1": {
          "name": "support.function.array.sugly"
        }
      }
    },
    {
      "name": "meta.function-object-type-sugly",
      "match": "(?:\\()\\s*(object)\\s+(assign|owns|fields-of)\\b",
      "captures": {
        "1": {
          "name": "storage.type.generic.sugly"
        },
        "2": {
          "name": "support.function.object.type.sugly"
        }
      }
    },
    {
      "name": "meta.function-class-type-sugly",
      "match": "(?:\\()\\s*(class)\\s+(attach)\\b",
      "captures": {
        "1": {
          "name": "storage.type.generic.sugly"
        },
        "2": {
          "name": "support.function.class.type.sugly"
        }
      }
    },
    {
      "name": "meta.function-class-sugly",
      "match": "(?:\\)|\\s)+(default|as|from|to-object|constructor|activator)\\b",
      "captures": {
        "1": {
          "name": "support.function.array.sugly"
        }
      }
    },
    {
      "name": "meta.function-iteration-sugly",
      "match": "\\b(iterate|iterator|select|selector|collect|collector|reduce|reducer)\\b",
      "captures": {
        "1": {
          "name": "support.function.iteration.sugly"
        }
      }
    },
    {
      "name": "meta.function-runtime-sugly",
      "match": "\\b(compile|compiler|tokenize|tokenizer|interpreter|eval|run)\\b",
      "captures": {
        "1": {
          "name": "support.function.runtime.sugly"
        }
      }
    },
    {
      "name": "meta.function-lib-sugly",
      "match": "\\b(encode|print|warn)\\b",
      "captures": {
        "1": {
          "name": "support.function.lib.sugly"
        }
      }
    },
    {
      "name": "meta.object-lib-sugly",
      "match": "\\b(uri|math|json)\\b",
      "captures": {
        "1": {
          "name": "support.variable.object.lib.sugly"
        }
      }
    },
    {
      "name": "meta.function-uri-lib-sugly",
      "match": "(?:\\()\\s*(uri)\\s+(encode|decode|escape|unescape)\\b",
      "captures": {
        "1": {
          "name": "support.variable.uri.lib.sugly"
        },
        "2": {
          "name": "support.function.uri.lib.sugly"
        }
      }
    },
    {
      "name": "meta.function-json-lib-sugly",
      "match": "(?:\\()\\s*(json)\\s+(of|parse)\\b",
      "captures": {
        "1": {
          "name": "support.variable.json.lib.sugly"
        },
        "2": {
          "name": "support.function.json.lib.sugly"
        }
      }
    },
    {
      "name": "meta.constant-math-lib-sugly",
      "match": "(?:\\()\\s*(math)\\s+(e|pi)\\b",
      "captures": {
        "1": {
          "name": "support.variable.math.lib.sugly"
        },
        "2": {
          "name": "support.constant.math.lib.sugly"
        }
      }
    },
    {
      "name": "meta.function-math-lib-sugly",
      "match": "(?:\\()\\s*(math)\\s+(\\w+)\\b",
      "captures": {
        "1": {
          "name": "support.variable.math.lib.sugly"
        },
        "2": {
          "name": "support.function.math.lib.sugly"
        }
      }
    },
    {
      "name": "meta.class-lib-sugly",
      "match": "\\b(emmiter|timer)\\b",
      "captures": {
        "1": {
          "name": "support.class.lib.sugly"
        }
      }
    },
    {
      "name": "meta.function-emitter-sugly",
      "match": "(?:\\)|\\s)+(on|off|emit)\\b",
      "captures": {
        "1": {
          "name": "support.function.emitter.sugly"
        }
      }
    },
    {
      "name": "meta.function-timer-class-sugly",
      "match": "(?:\\()\\s*(timer)\\s+(timeout)\\b",
      "captures": {
        "1": {
          "name": "support.class.timer.sugly"
        },
        "2": {
          "name": "support.function.timer.class.sugly"
        }
      }
    },
    {
      "name": "meta.function-timer-sugly",
      "match": "(?:\\)|\\s)+(start|stop)\\b",
      "captures": {
        "1": {
          "name": "support.function.timer.sugly"
        }
      }
    },
    {
      "name": "meta.function-class-sugly",
      "match": "(?:\\)|\\s)+(default|as|from|to-object|constructor|activator)\\b",
      "captures": {
        "1": {
          "name": "support.function.array.sugly"
        }
      }
    },
    {
      "name": "meta.function-test-sugly",
      "match": "\\b(define|should|test)\\b",
      "captures": {
        "1": {
          "name": "support.function.test.sugly"
        }
      }
    },
    {
      "name": "meta.operator-test-sugly",
      "match": "\\b(assert)\\b",
      "captures": {
        "1": {
          "name": "keyword.other.assert.test.sugly"
        }
      }
    },
    {
      "name": "constant.numeric.oct.sugly",
      "match": "\\b(0[0-7]+)\\b"
    },
    {
      "name": "constant.numeric.hex.sugly",
      "match": "\\b(0x[0-9a-fA-F]+)\\b"
    },
    {
      "name": "constant.numeric.binary.sugly",
      "match": "\\b(0b[01]+)\\b"
    },
    {
      "name": "constant.numeric.decimal.sugly",
      "match": "\\b((([0-9]+\\.?[0-9]*)|(\\.[0-9]+))((e|E)(\\+|-)?[0-9]+)?)\\b"
    },
    {
      "begin": "\"",
      "beginCaptures": {
        "0": {
          "name": "punctuation.definition.string.begin.sugly"
        }
      },
      "end": "\"",
      "endCaptures": {
        "0": {
          "name": "punctuation.definition.string.end.sugly"
        }
      },
      "name": "string.quoted.double.sugly",
      "patterns": [
        {
          "match": "\\\\.",
          "name": "constant.character.escape.sugly"
        }
      ]
    }
  ]
}
